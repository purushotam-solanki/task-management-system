{
  "compilerOptions": {
    "baseUrl": "./",
    "paths": {
      "@src/*": [
        "src/*"
      ],
      "@lib/*": [
        "src/lib/*"
      ],
      "@utils": [
        "src/lib/utils/*"
      ],
      "@controllers": [
        "src/controllers/*"
      ],
      "@services": [
        "src/services/*"
      ],
      "@routes":[
        "src/routes/*"
      ]
      
    }
  },
  "exclude": [
    "node_modules"
  ]
}